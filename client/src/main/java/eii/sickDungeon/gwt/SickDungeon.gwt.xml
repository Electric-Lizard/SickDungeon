<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="SickDungeon">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.realityforge.gwt.websockets.WebSockets"/>
    <inherits name="com.google.web.bindery.autobean.AutoBean"/>
    <inherits name="com.google.gwt.json.JSON"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='eii.sickDungeon.gwt.client.SickDungeon'/>
    
    <set-configuration-property name="CssResource.obfuscationPrefix" value="empty"/>

    <source path="client"/>
    <source path="shared"/>
</module>